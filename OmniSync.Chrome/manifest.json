{
  "manifest_version": 3,
  "name": "OmniSync Browser Controller",
  "version": "1.0",
  "permissions": [
    "tabs",
    "scripting",
    "storage",
    "history"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "http://10.0.0.37/*",
    "http://127.0.0.1/*",
    "<all_urls>"
  ]
}
